#include <iostream>
#include<algorithm>
using namespace std;
int main() {
    vector <int> vb={30,12,45,23,10};
    sort(vb.begin(),vb.end());
   for(auto it:vb)
        cout<<it<<" ";
    cout<<"\n";
    sort(vb.begin(),vb.end(),greater <int> ());
    for(auto it:vb)
        cout<<it<<" ";

return 0;
}
