#include <iostream>
#include<algorithm>
using namespace std;
int n;
void bubblesort(int arr[]){
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-1;j++){
            if(arr[j]>arr[j+1]){
                swap(arr[j],arr[j+1]);
            }
        }
    }
}
int main() {
   cin>>n;
   int arr[n];
   for(int i=0;i<n;i++)
   cin>>arr[i];
   bubblesort(arr);
   for(int i=0;i<n;i++)
    cout<<arr[i]<<" ";
return 0;
}
